<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<meta content="always" name="referrer">
<link rel="icon" id="myid" href="../static/img/avatar.png">
<link rel="apple-touch-icon" id="myid" href="../static/img/avatar.png">
<title>询证检索</title>

<link rel="stylesheet" type="text/css"
	href="../static/section4/index.css">

<link rel="stylesheet" type="text/css"
	href="../static/section4/main.css">

</head>
<body class="is-lite has-background s-manhattan-index"
	data-gr-c-s-loaded="true" style="">
	<div id="wrapper">
		<div class="s-skin-container s-isindex-wrap"
			style="background-color:#ededed;"></div>
		<div id="head" class="s-skin-hasbg white-logo s-opacity-25">
			<div class="clear"></div>
			<div id="head_wrapper"
				class="s-isindex-wrap head_wrapper s-title-img  s-ps-islite">
				<div id="s_fm" class="s_form">
					<div class="s_form_wrapper soutu-env-mac soutu-env-newindex"
						id="s_form_wrapper">
						<div id="lg" class="s-p-top">
							<img id="s_lg_img_new" class="s_lg_img_gold_showre"
								src="../static/section4_2/search.png" width="100" height="100"
								usemap="#mp" title="" cursor="default">
							<map name="mp" id="s_mp">
								<area style="cursor:pointer;outline:none;" shape="rect"
									coords="0,0,270,129"
									onmousedown="return ns_c({&#39;fm&#39;:&#39;behs&#39;,&#39;tab&#39;:&#39;bdlogo&#39;})">
							</map>
						</div>


						<!-- 搜索-->
						<form name="f" id="form" action="" class="fm">
							<span id="s_kw_wrap"
								class="bg s_ipt_wr quickdelete-wrap iptfocus"><input
								type="text" class="s_ipt" name="search" id="kw" maxlength="100"
								autocomplete="off"></span><input type="submit" value="询证检索"
								id="su" class="btn self-btn bg s_btn" style="color: #fff;">
						</form>
					</div>
				</div>
			</div>
		</div>
	</div>
</body>
</html>
